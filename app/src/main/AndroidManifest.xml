<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pp.inzynierka.douslug">

    <application
        android:name="pp.inzynierka.douslug.db.DBConnector"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".calendar.CalendarWeekActivity" />
        <activity android:name=".calendar.CalendarDayActivity" />
        <activity android:name=".db.DBTestActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".calendar.CalendarMonthActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>